/* ==========================================================================
   3. LAYOUT (from layout.css)
   ========================================================================== */

html {
  --plyr-color-main: #C8102E;
}

.container {
	margin: 20px auto;
	max-width: 500px;
}

@media only screen and (orientation:portrait)
{
  .wrapper{
    transform-origin: 50% 50%;
    -webkit-transform: rotate(90deg);
    -moz-transform: rotate(90deg);
    -o-transform: rotate(90deg);
    -ms-transform: rotate(90deg);
    transform: rotate(90deg);
    transform-origin:bottom left;

    position:absolute;
    top:-100vw;
    height:100vw;
    width:100vh;


    background-color:rgba(250,250,250,1);
    color:#fff;
  }

  .slide {
    height:100vmin;
    width:100vmax;
  }

  .step .slide {
    height:100vmin;
    width:100vmax;
  }

  .impressdiv {
    -webkit-transform: rotate(90deg);
    -moz-transform: rotate(90deg);
    -o-transform: rotate(90deg);
    -ms-transform: rotate(90deg);
    transform: rotate(90deg);
    height:100vmin;
    width:100vmax;
  }


  /* .slide {
  width: 100vh;
  width: 100vw;
  } */

}

@media only screen and (orientation:landscape){
  .wrapper {
    /* transform-origin: 50% 50%; */
    -webkit-transform: rotate(0deg);
    -moz-transform: rotate(0deg);
    -o-transform: rotate(0deg);
    -ms-transform: rotate(0deg);
    transform: rotate(0deg);
    background-color:rgba(250,250,250,1);
    color:#fff;

    position:absolute;
    /* top:0;
    top:0; */
    height:100vh;
    width:100vw;
  }

  /* .slide {
  transform-origin:top left;
  position:absolute;
  top:0vw;
  left: 5vw;
  height:  90vh;
  width: 30vw;
  } */

  .impressdiv {
    -webkit-transform: rotate(0deg);
    -moz-transform: rotate(0deg);
    -o-transform: rotate(0deg);
    -ms-transform: rotate(0deg);
    transform: rotate(0deg);
    height:100vmin;
    width:100vmax;
  }

  .slide {
    height:100vmin;
    width:100vmax;
  }

  .step .slide {
    height:100vmin;
    width:100vmax;
  }

}

